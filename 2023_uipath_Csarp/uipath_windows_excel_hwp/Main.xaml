    <ui:InvokeCode ContinueOnError="{x:Null}" Code="string FilePath = @inFilePath;&#xD;&#xA;string FilePath2 = @inFilePath2;&#xD;&#xA;DataTable DT = inDT;&#xD;&#xA;//System.Console.WriteLine(FilePath);&#xD;&#xA;&#xD;&#xA;try&#xD;&#xA;{&#xD;&#xA;//레지스트리에 경로 설정해주기&#xD;&#xA;string Str_M_Path = @&quot;C:\hwp_보안모듈_Automation\보안모듈(Automation)\FilePathCheckerModuleExample.dll&quot;;&#xD;&#xA;string HNCRootkey = @&quot;HKEY_CURRENT_USER\Software\HNC\HwpAutomation\Modules&quot;;&#xD;&#xA;&#xD;&#xA;Microsoft.Win32.Registry.SetValue(HNCRootkey,&quot;FilePathCheckerModuleExample&quot;,Str_M_Path);&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;    HwpObjectLib.IHwpObject hwp = new HwpObjectLib.HwpObject();&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;hwp.RegisterModule(&quot;FilePathCheckDLL&quot;,&quot;FilePathCheckerModuleExample&quot;);&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;//화면 (안)보이기 false True dynamic 으로 동적으로 처리 하기&#xD;&#xA;&#x9;dynamic xHwpWin = hwp.XHwpWindows;&#xD;&#xA;&#x9;dynamic activeXHwpWin = xHwpWin.Active_XHwpWindow;&#xD;&#xA;&#x9;activeXHwpWin.Visible = false;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;//화면열기&#xD;&#xA;    hwp.Open(FilePath, &quot;&quot;, &quot;&quot;);&#xD;&#xA;&#x9;//전체화면&#xD;&#xA;&#x9;//hwp.Run(&quot;FrameFullScreen&quot;);&#xD;&#xA;&#x9;//필드 리스트로 출력하기 2가 갯수, 0이 아무것도 없는 거&#xD;&#xA;&#x9;string Str_Fields = hwp.GetFieldList(0,&quot;&quot;);&#xD;&#xA;&#x9;string[] A_str_field = Str_Fields.Split('\x02');&#xD;&#xA;&#x9;Console.WriteLine(&quot;출력 A_str_field : &quot; + string.Join(&quot;,&quot;, A_str_field));&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;//문서 현재쪽 전체선택 후 복사해서 마지막쪽으로 가서 붙여넣기&#xD;&#xA;&#x9;//hwp.MovePos(0, 0, 0);&#xD;&#xA;&#x9;hwp.Run(&quot;SelectAll&quot;);&#xD;&#xA;&#x9;hwp.Run(&quot;Copy&quot;);&#xD;&#xA;&#x9;hwp.MovePos(3,0,0);&#xD;&#xA;&#x9;foreach (int i in Enumerable.Range(0,DT.Rows.Count-1))&#xD;&#xA;&#x9;{&#xD;&#xA;&#x9;    //Console.WriteLine(DT.Rows[i+1][0].ToString());&#xD;&#xA;&#x9;    hwp.Run(&quot;Paste&quot;);&#xD;&#xA;&#x9;    hwp.MovePos(3, 0, 0);&#xD;&#xA;&#x9;}&#xD;&#xA;&#x9;//문서 제일 위로 런타임을 짧게 하기 위해서&#xD;&#xA;&#x9;hwp.MovePos(0, 0, 0);&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;//필드에 내용바꿔서 넣어주기&#xD;&#xA;&#x9;foreach (int i in Enumerable.Range(0, DT.Rows.Count))&#xD;&#xA;&#x9;{&#xD;&#xA;&#x9;    foreach (string field in A_str_field)&#xD;&#xA;&#x9;    {&#xD;&#xA;&#x9;        hwp.PutFieldText($&quot;{field}{{{{{i}}}}}&quot;, DT.Rows[i][field].ToString());&#xD;&#xA;&#x9;    }&#xD;&#xA;&#x9;}&#xD;&#xA;&#x9;//저장&#xD;&#xA;&#x9;hwp.SaveAs(FilePath2,&quot;&quot;,&quot;&quot;);&#xD;&#xA;&#x9;//종료&#xD;&#xA;    hwp.Quit();&#xD;&#xA;&#x9;//객체해제&#xD;&#xA;&#x9;System.Runtime.InteropServices.Marshal.ReleaseComObject(hwp);&#xD;&#xA;&#x9;&#xD;&#xA;}&#xD;&#xA;catch (Exception ex)&#xD;&#xA;{&#xD;&#xA;    Console.WriteLine($&quot;An error occurred: {ex.Message}&quot;);&#xD;&#xA;}&#xD;&#xA;" DisplayName="dynamic사용한거_Invoke Code" sap:VirtualizedContainerService.HintSize="542.4,164.8" sap2010:WorkflowViewState.IdRef="InvokeCode_1" Language="CSharp">
